{
  "kind": "collectionType",
  "collectionName": "stories",
  "info": {
    "singularName": "story",
    "pluralName": "stories",
    "displayName": "Story",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "components": {
      "type": "dynamiczone",
      "components": [
        "modules.pullquote",
        "modules.text-module",
        "modules.subtitle",
        "modules.title-module",
        "modules.image-carousel",
        "modules.image",
        "modules.table-module",
        "modules.buttons-module",
        "modules.grid-module"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "timeframe": {
      "displayName": "timeframe",
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "core.timeframe"
    },
    "locations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::location.location",
      "mappedBy": "stories"
    },
    "publicationDate": {
      "displayName": "Publication Date",
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "core.publication-date"
    },
    "storyLinks": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "modules.buttons-module"
    },
    "shortDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "maxLength": 250
    },
    "triplyRecords": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::triply-record.triply-record",
      "inversedBy": "stories"
    },
    "themes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::theme.theme",
      "inversedBy": "stories"
    }
  }
}
